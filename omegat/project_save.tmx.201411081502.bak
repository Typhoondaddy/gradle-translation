    <tu>
      <tuv lang="EN-US">
        <seg>Declarative builds and build-by-convention</seg>
      </tuv>
      <tuv lang="ZH-CN" changeid="msdx" changedate="20141101T034420Z" creationid="msdx" creationdate="20141101T034420Z">
